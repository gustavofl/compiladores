programa {
	funcao real soma(inteiro a, inteiro b) {
		escreva(a)
		retorne a+b
	}

	funcao vv(inteiro p) {
		se(p<5) {
			leia(p)
		} senao {
			enquanto(p>3)
				p=p/(4+2)+5*7-22
		}
	}

	funcao inicio () {
		inteiro k
		k=soma(3,7)
		leia(k)
		vv(k)
		inteiro lista[10]
		inteiro i = 0
		enquanto(i<10 e lista[i] >= 11) {
			lista[i] = 17%i
		}
	}
}